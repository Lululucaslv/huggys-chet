{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "api/agent/chat.js": {
      "maxDuration": 60
    },
    "api/agent/chat-simple.js": {
      "maxDuration": 60
    },
    "api/agent/chat-minimal.js": {
      "maxDuration": 60
    },
    "api/agent/chat-simplified.js": {
      "maxDuration": 60
    },
    "api/agent/chat-debug.js": {
      "maxDuration": 60
    },
    "api/setup/add-test-data.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/agent/chat",
      "destination": "/api/agent/chat-simple.js"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
