{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "functions": {
    "api/chat/index.js": { "maxDuration": 60 },
    "api/agent/chat.js": { "maxDuration": 60 },
    "api/agent/chat-simple.js": { "maxDuration": 60 },
    "api/agent/chat-minimal.js": { "maxDuration": 60 },
    "api/agent/chat-simplified.js": { "maxDuration": 60 },
    "api/agent/chat-debug.js": { "maxDuration": 60 },
    "api/bookings/list.js": { "maxDuration": 60 },
    "api/bookings/create.js": { "maxDuration": 60 },
    "api/bookings/summary.js": { "maxDuration": 60 },
    "api/setup/add-test-data.js": { "maxDuration": 60 }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
